        <details key="requiredInterfacesAreDelegatedOnce" value="&#xA;&#x9;&#x9;&#x9;requiredInterfaces->asBag() = delegationconnectors->select(delegationType = DelegationType::Required)->collect(interface)->asBag()"/>
        <details key="delegationsReferenceDirectElements" value="&#xA;&#x9;&#x9;&#x9;&#x9;elements->includesAll(delegationconnectors->collect(assemblyContext))"/>
        <details key="assemblyConnectorsReferenceDirectElements" value="&#xA;&#x9;&#x9;&#x9;&#x9;elements->includesAll(assemblyconnectors->collect(consumer)->union(assemblyconnectors->collect(provider)))"/>
      </eAnnotations>
